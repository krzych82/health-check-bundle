{
    "name": "anera/health-check",
    "description": "Anera HealthCheck Bundle provides an endpoint to check if your application works properly.",
    "license": "MIT",
    "authors": [
        {
            "name": "Krzysztof Drużyński",
            "email": "aneradevelopment@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "symfony/config": ">=3.4",
        "symfony/http-foundation": ">=3.4",
        "symfony/http-kernel": ">=3.4",
        "symfony/dependency-injection": ">=3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Anera\\HealthCheck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anera\\HealthCheck\\Tests\\": "tests/"
        }
    }
}
